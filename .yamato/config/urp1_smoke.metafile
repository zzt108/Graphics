# smoke tests should run befor every git commit/push
# smoke tests are testing both edit mode and play mode unit tests, but only on Win platform to save time
project:
  name: URP_1_Smoke
  folder: urp1
test_platforms:
  - type: playmode
  - type: editmode
  - type: Standalone
platforms:
  - name: Win
    apis:
      - DX11
all:
  dependencies:
    - platform: Win
      api: DX11
      test_platforms:
        - playmode
        - editmode
        - Standalone
